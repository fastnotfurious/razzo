        </tree>
        <parameter id="BDBirthRate.t:mbd" lower="0.0" name="stateNode" upper="10000.0">1.0</parameter>
        <parameter id="BDDeathRate.t:mbd" lower="0.0" name="stateNode" upper="1.0">0.5</parameter>
        <parameter id="clockRate.c:mbd" name="stateNode">1.0</parameter>
    </state>

    <init id="RandomTree.t:mbd" spec="beast.evolution.tree.RandomTree" estimate="false" initial="@Tree.t:mbd" taxa="@mbd">
