            </prior>
            <prior id="DeathRatePrior.t:mbd" name="distribution" x="@BDDeathRate.t:mbd">
                <Uniform id="Uniform.101" name="distr" upper="Infinity"/>
            </prior>
            <prior id="ClockPrior.c:mbd" name="distribution" x="@clockRate.c:mbd">
